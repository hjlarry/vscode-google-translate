{
	"name": "google-translate",
	"displayName": "google translate",
	"description": "a google translate for chinese user view source code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.translate"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.translate",
				"title": "Google Translate"
			}
		],
		"keybindings": [
			{
				"command": "extension.translate",
				"key": "ctrl+t",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"yo": "^2.0.5"
	}
}
